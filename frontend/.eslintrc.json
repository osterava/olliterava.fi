{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "double"],
      "react/prop-types": "off",
      "no-undef": "off"
    }
  }
  